package q1;

import java.io.*;
import java.util.*;
public class Main {
	public static void main(String[] args)throws IOException {
		BufferedReader in=new BufferedReader(new InputStreamReader(System.in));
		String[] x=in.readLine().trim().split(" ");
		int n=Integer.parseInt(x[0]);
		int m=Integer.parseInt(x[0]);
		String s=in.readLine();
		for(int i=0;i<m;i++){
			x=in.readLine().trim().split(" ");
			char a=x[0].charAt(0);
			char b=x[1].charAt(0);
			int[] change=new int[n];
			for(int j=0;j<n;j++){
				if(s.charAt(j)==a){
					s=s.substring(0,i)+b+s.substring(i+1);
					change[j]=1;
				}
			}
			for(int j=0;j<n;j++){
				if(s.charAt(j)==b && change[j]==0){
					s=s.substring(0,i)+a+s.substring(i+1);
					change[j]=1;
				}
			}
			System.out.println(s);
		}
		System.out.println(s);
	}

}
